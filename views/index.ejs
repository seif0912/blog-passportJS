<%- include('components/head.ejs', {page: 'feed', cssSource: './css/style.css'}); %>

<body>
  <%- include('components/header.ejs', {isLoggedIn}); %>
  <%- include('components/hero.ejs' )%>

  <div class="posts">
    <% posts.forEach(post => { %>
    <div class="post">
      <h3 class="title"><%= post.title %> </h3>
      <p class="body"><%= post.body %> </p>
    </div>
    <% }) %>
  </div>
</body>

</html>