<%- include('components/head.ejs', {page: 'feed'}); %>

<body>
  <%- include('components/header.ejs', {isLoggedIn}); %>

  <div class="posts">
    <% posts.forEach(post => { %>
    <div class="post">
      <h3 class="title"><%= post.title %> </h3>
      <p class="body"><%= post.body %> </p>
    </div>
    <% }) %>
  </div>
</body>

</html>